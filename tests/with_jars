spark-submit-flamegraph --jars /path/to/mycool.jar,/path/to/another.jar
spark-submit --jars $HOME/.spark-flamegraph/statsd-jvm-profiler.jar,/path/to/mycool.jar,/path/to/another.jar --conf spark.executor.extraJavaOptions=-javaagent:statsd-jvm-profiler.jar=server=.*,port=.*,reporter=InfluxDBReporter,database=profiler,username=profiler,password=profiler,prefix=sparkapp,tagMapping=spark --driver-java-options -javaagent:$HOME/.spark-flamegraph/statsd-jvm-profiler.jar=server=.*,port=.*,reporter=InfluxDBReporter,database=profiler,username=profiler,password=profiler,prefix=sparkapp,tagMapping=spark
